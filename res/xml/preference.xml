    <PreferenceCategory android:title="@string/dialog_based_phonePrefs" >
        <CheckBoxPreference
            android:key="INCALL"
            android:defaultValue="true"
            android:summary="@string/summary_checkbox_INCALL"
            android:title="@string/title_checkbox_INCALL" />
        <CheckBoxPreference
            android:key="OUTCALL"
            android:defaultValue="true"
            android:summary="@string/summary_checkbox_OUTCALL"
            android:title="@string/title_checkbox_OUTCALL" />
        <CheckBoxPreference
            android:key="SPEAK"
            android:defaultValue="false"
            android:summary="@string/summary_checkbox_speaker"
            android:title="@string/title_checkbox_speaker" />
    </PreferenceCategory>
    
    <PreferenceCategory android:title="@string/dialog_based_phonePrefs_ergo" >
        <CheckBoxPreference
