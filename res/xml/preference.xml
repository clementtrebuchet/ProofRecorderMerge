        <CheckBoxPreference
            android:key="UAC_ASSISTED"
            android:defaultValue="true"
            android:summary="@string/summary_checkbox_UAC_ASSISTED"
            android:title="@string/title_checkbox_UAC_ASSISTED" />
    </PreferenceCategory>
    
    <PreferenceCategory android:title="@string/dialog_based_phonePrefs_clean" >
        <ListPreference
            android:defaultValue="NEVER"
            android:dialogTitle="@string/dlgtitle_list_auto_clean"
            android:key="auto_clean"
            android:summary="@string/summary_list_auto_clean"
            android:title="@string/title_list_auto_clean" />
    </PreferenceCategory>
    
